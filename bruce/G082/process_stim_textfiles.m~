for i = 1:length(file_ids)
    fprintf('Parsing file rls.rc%d\n',file_ids(i));
    
    [trial_id,se_id,trial_nums,frame_nums,left_pix,right_pix] = parse_random_bar_file(sprintf('rls.rc%d',file_ids(i)));
    all_trial_ids = [all_trial_ids; trial_id];
    all_se_ids = [all_se_ids; se_id];
    all_file_ids = [all_stimblock_ids; ones(length(all_trial_ids),1)*i];
    all_frame_nums = [all_frame_nums; 
    all_trial_nums = [all_tnums; (1:length(trial_ids)
    
    all_left_pix = cat(1,all_left_pix, {left_pix});
    all_right_pix = cat(1, all_right_pix, {right_pix});
end

